<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jenny.anothertry.MainActivity">

    <Button
        android:id="@+id/buttonBasic"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="@string/title_activity_basic"
        style="@style/Button"
        tools:layout_editor_absoluteY="33dp"
        tools:layout_editor_absoluteX="34dp" />

    <Button
        android:id="@+id/buttonAsynchronous"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="@string/title_activity_asynchronous"
        style="@style/Button"
        tools:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="148dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/buttonBasic"
        android:layout_marginStart="85dp" />

</RelativeLayout>
